<!-- CAROUSEL -->
<div class="carousel fade-carousel slide" data-ride="carousel" data-interval="4000" id="bs-carousel">
  <!-- Overlay -->
  <div class="overlay"></div>

  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#bs-carousel" data-slide-to="0" class="active"></li>
    <li data-target="#bs-carousel" data-slide-to="1"></li>
    <li data-target="#bs-carousel" data-slide-to="2"></li>
    <li data-target="#bs-carousel" data-slide-to="3"></li>
    <li data-target="#bs-carousel" data-slide-to="4"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <div class="item slides active">
      <div class="slide-1"></div>
      <div class="hero">
        <hgroup>
            <h1>Heroes of Hong Kong</h1>
            <h3>Professor Jim Chi-yung</h3>
            <h3>#1 hero for the environment</h3>
        </hgroup>
        <button id="carousel-button-1" class="btn btn-hero btn-lg" role="button">See all environmental heroes</button>
      </div>
    </div>
    <div class="item slides">
      <div class="slide-2"></div>
      <div class="hero">
        <hgroup>
            <h1>Heroes of Hong Kong</h1>
            <h3>Eric Chen Zixiang</h3>
            <h3>#1 hero for the community</h3>
        </hgroup>
        <button id="carousel-button-2"class="btn btn-hero btn-lg" role="button">See all community heroes</button>
      </div>
    </div>
    <div class="item slides">
      <div class="slide-3"></div>
      <div class="hero">
        <hgroup>
            <h1>Heroes of Hong Kong</h1>
            <h3>Trisha Tran</h3>
            <h3>#1 hero for the youth</h3>
        </hgroup>
        <button id="carousel-button-3"class="btn btn-hero btn-lg" role="button">See all youth heroes</button>
      </div>
    </div>
    <div class="item slides">
      <div class="slide-4"></div>
      <div class="hero">
        <hgroup>
            <h1>Heroes of Hong Kong</h1>
            <h3>In my neighbourhood</h3>
            <h3>Find the heroes in your hood!</h3>
        </hgroup>
        <button id="carousel-button-4" class="btn btn-hero btn-lg" role="button">In my Neighbourhood</button>
      </div>
    </div>
    <div class="item slides">
      <div class="slide-5"></div>
      <div class="hero">
        <hgroup>
            <h1>Heroes of Hong Kong</h1>
            <h3>Most Popular</h3>
            <h3>Across all categories</h3>
        </hgroup>
        <button id="carousel-button-5" class="btn btn-hero btn-lg" role="button">See all popular heroes</button>
      </div>
    </div>
  </div>
</div>

